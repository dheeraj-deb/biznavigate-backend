# Database
DATABASE_URL=postgresql://user:password@localhost:5432/biznavigate

# Redis
REDIS_HOST=localhost
REDIS_PORT=6379

# Zoho (optional)
ZOHO_CLIENT_ID=your_zoho_client_id
ZOHO_CLIENT_SECRET=your_zoho_client_secret
